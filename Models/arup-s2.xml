<?xml version="1.0" encoding="ISO-8859-1"?>

<PropertyList>
  
  <path>arup-s2.ac</path>
  
  <nasal>
    <load>
      var livery_update = aircraft.livery_update.new("Aircraft/Arup-S2/Models/Liveries", 10);
      
      var self = cmdarg();
      
      var float_0 = self.getNode("sim/multiplay/generic/float[0]", 1);
      var float_1 = self.getNode("sim/multiplay/generic/float[1]", 1);
      
      var crew_door = self.getNode("sim/model/door-positions/crew/position-norm", 1);
      var passenger_door = self.getNode("sim/model/door-positions/passenger/position-norm", 1);
      
      crew_door.alias(float_0);
      passenger_door.alias(float_1);
    </load>
    
    <unload>
      livery_update.stop();
      crew_door.unalias();
      passenger_door.unalias();
    </unload>
  </nasal>
  
  <animation>
    <!-- Objets opaques -->
    <object-name>fuselage</object-name>
    <object-name>ailes</object-name>
    <object-name>boutdailes</object-name>
    <object-name>volet</object-name>
    <object-name>derive</object-name>
    <object-name>direction</object-name>
    <object-name>cacheG</object-name>
    <object-name>cacheD</object-name>
    <object-name>axeG</object-name>
    <object-name>axeD</object-name>
    <object-name>bol</object-name>
    <object-name>flaperonG</object-name>
    <object-name>flaperonD</object-name>
    <object-name>tourvitres</object-name>
    
    <object-name>trous</object-name>
    <object-name>moteur</object-name>
    <object-name>bequille</object-name>
    <object-name>helice</object-name>
    <object-name>intcacheG</object-name>
    <object-name>intcacheD</object-name>
    <object-name>roueG</object-name>
    <object-name>roueD</object-name>
    
    <!-- Objets transparents -->
    <object-name>propdisc</object-name>
    <object-name>propblur</object-name>
    <object-name>vitres</object-name>
  </animation>
  
  <animation>
    <type>material</type>
    <object-name>fuselage</object-name>
    <object-name>ailes</object-name>
    <object-name>boutdailes</object-name>
    <object-name>volet</object-name>
    <object-name>derive</object-name>
    <object-name>direction</object-name>
    <object-name>cacheG</object-name>
    <object-name>cacheD</object-name>
    <object-name>axeG</object-name>
    <object-name>axeD</object-name>
    <object-name>bol</object-name>
    <object-name>flaperonG</object-name>
    <object-name>flaperonD</object-name>
    <object-name>tourvitres</object-name>
    
    <object-name>trous</object-name>
    <object-name>moteur</object-name>
    <object-name>bequille</object-name>
    <object-name>helice</object-name>
    <object-name>intcacheG</object-name>
    <object-name>intcacheD</object-name>
    <object-name>roueG</object-name>
    <object-name>roueD</object-name>
    
    <property-base>sim/model/livery</property-base>
    <texture-prop>texture</texture-prop>
    <texture>texture.png</texture>
    <specular>
      <red-prop>specular/red</red-prop>
      <green-prop>specular/green</green-prop>
      <blue-prop>specular/blue</blue-prop>
    </specular>
  </animation>
  
  <effect>
    <inherits-from>Aircraft/Generic/Effects/Fuselagereflect</inherits-from>
    <object-name>fuselage</object-name>
    <object-name>ailes</object-name>
    <object-name>boutdailes</object-name>
    <object-name>volet</object-name>
    <object-name>derive</object-name>
    <object-name>direction</object-name>
    <object-name>cacheG</object-name>
    <object-name>cacheD</object-name>
    <object-name>axeG</object-name>
    <object-name>axeD</object-name>
    <object-name>bol</object-name>
    <object-name>flaperonG</object-name>
    <object-name>flaperonD</object-name>
    <object-name>tourvitres</object-name>
  </effect>
  
  <!-- working for Rembrandt -->
  <effect>
    <inherits-from>Aircraft/Arup-S2/Models/Effects/Glass/glass</inherits-from>
    <object-name>vitres</object-name>
  </effect>
  
  <!-- working for Rembrandt -->
  <effect>
    <inherits-from>Effects/model-transparent</inherits-from>
    <object-name>vitres</object-name>
    <object-name>propdisc</object-name>
    <object-name>propblur</object-name>
  </effect>
  
  <!-- Une ombre pour OSG -->
  <model>
    <path>Aircraft/Arup-S2/Models/shadow.xml</path>
    <offsets>
      <x-m>  0.00 </x-m>
      <y-m>  0.00 </y-m>
      <z-m> -1.12 </z-m>
    </offsets>
  </model>
  
  <!-- Interieur -->
  <model>
    <usage>interior</usage>
    <path>Aircraft/Arup-S2/Models/Interior/interior.xml</path>
  </model>
  
  <!-- Cranking effects -->
  <!-- Left -->
  <model>
    <path>Aircraft/Arup-S2/Models/Effects/cranking/cranking.xml</path>
    <offsets>
      <x-m> -2.000 </x-m>
      <y-m> -0.611 </y-m>
      <z-m> -0.163 </z-m>
    </offsets>
  </model>
  
  <!-- Right -->
  <model>
    <path>Aircraft/Arup-S2/Models/Effects/cranking/cranking.xml</path>
    <offsets>
      <x-m> -2.000 </x-m>
      <y-m>  0.611 </y-m>
      <z-m> -0.163 </z-m>
    </offsets>
  </model>
  
  <!-- Effets sur le sol -->
  <!-- Roue gauche -->
  <model>
    <path>Aircraft/Arup-S2/Models/Effects/ground/ground0.xml</path>
    <offsets>
      <x-m> -0.933 </x-m>
      <y-m> -0.777 </y-m>
      <z-m> -1.316 </z-m>
    </offsets>
  </model>
  
  <!-- Roue droite -->
  <model>
    <path>Aircraft/Arup-S2/Models/Effects/ground/ground1.xml</path>
    <offsets>
      <x-m> -0.933 </x-m>
      <y-m>  0.777 </y-m>
      <z-m> -1.316 </z-m>
    </offsets>
  </model>
  
  <!-- Patte de queue -->
  <model>
    <path>Aircraft/Arup-S2/Models/Effects/ground/ground2.xml</path>   
    <offsets>
      <x-m>  1.574 </x-m>
      <y-m>  0.000 </y-m>
      <z-m> -0.786 </z-m>
    </offsets>
  </model>
  
  <!-- L'hélice -->
  <animation>
    <name>HeliceComplete</name>
    <object-name>helice</object-name>
    <object-name>bol</object-name>
    <object-name>propdisc</object-name>
    <object-name>propblur</object-name>
  </animation>
  
  <!-- Séléction de l'objet visible en fonction des rpm -->
  <animation>
    <type>select</type>
    <object-name>propdisc</object-name>
    <condition>
      <greater-than>
        <property>engines/engine[0]/rpm</property>
        <value>900</value>
      </greater-than>
    </condition>
  </animation>
  
  <animation>
    <type>select</type>
    <object-name>propblur</object-name>
    <condition>
      <and>
        <greater-than>
          <property>engines/engine[0]/rpm</property>
          <value>300.000</value>
        </greater-than>
        <less-than>
          <property>engines/engine[0]/rpm</property>
          <value>900.001</value>
        </less-than>
      </and>
    </condition>
  </animation>
  
  <animation>
    <type>select</type>
    <object-name>helice</object-name>
    <condition>
      <less-than>
        <property>engines/engine[0]/rpm</property>
        <value>900</value>
      </less-than>
    </condition>
  </animation>
  
  <animation>
    <type>spin</type>
    <object-name>HeliceComplete</object-name>
    <condition>
      <greater-than>
        <property>engines/engine[0]/rpm</property>
        <value> 5 </value>
      </greater-than>
    </condition>
    <property>engines/engine[0]/rpm</property>
    <factor> 1 </factor>
    <center>
      <x-m> -2.237 </x-m>
      <y-m>  0.000 </y-m>
      <z-m> -0.160 </z-m>
    </center>
    <axis>
      <x> -1 </x>
      <y>  0 </y>
      <z>  0 </z>
    </axis>
  </animation>
  
  <!-- Animations des flaperons -->
  <animation>
    <type>rotate</type>
    <object-name>flaperonG</object-name>
    <property>surface-positions/left-aileron-pos-norm</property>
    <interpolation>
      <entry><ind> -0.75 </ind><dep> -15 </dep></entry>
      <entry><ind>  0.00 </ind><dep>   0 </dep></entry>
      <entry><ind>  0.75 </ind><dep>  15 </dep></entry>
    </interpolation>
    <axis>
      <x1-m>  1.40612 </x1-m>
      <y1-m> -1.98334 </y1-m>
      <z1-m> -0.22685 </z1-m>
      <x2-m>  1.51934 </x2-m>
      <y2-m> -0.79660 </y2-m>
      <z2-m> -0.17577 </z2-m>
    </axis>
  </animation>
  
  <animation>
    <type>rotate</type>
    <object-name>flaperonD</object-name>
    <property>surface-positions/right-aileron-pos-norm</property>
    <interpolation>
      <entry><ind> -0.75 </ind><dep> -15 </dep></entry>
      <entry><ind>  0.00 </ind><dep>   0 </dep></entry>
      <entry><ind>  0.75 </ind><dep>  15 </dep></entry>
    </interpolation>
    <axis>
      <x1-m>  1.51934 </x1-m>
      <y1-m>  0.79660 </y1-m>
      <z1-m> -0.17577 </z1-m>
      <x2-m>  1.40612 </x2-m>
      <y2-m>  1.98334 </y2-m>
      <z2-m> -0.22685 </z2-m>
    </axis>
  </animation>
  
  <animation>
    <type>rotate</type>
    <object-name>flaperonG</object-name>
    <property>surface-positions/elevator-pos-norm</property>
    <interpolation>
      <entry><ind> -0.75 </ind><dep> -15 </dep></entry>
      <entry><ind>  0.00 </ind><dep>   0 </dep></entry>
      <entry><ind>  0.75 </ind><dep>  15 </dep></entry>
    </interpolation>
    <axis>
      <x1-m>  1.40612 </x1-m>
      <y1-m> -1.98334 </y1-m>
      <z1-m> -0.22685 </z1-m>
      <x2-m>  1.51934 </x2-m>
      <y2-m> -0.79660 </y2-m>
      <z2-m> -0.17577 </z2-m>
    </axis>
  </animation>
  
  <animation>
    <type>rotate</type>
    <object-name>flaperonD</object-name>
    <property>surface-positions/elevator-pos-norm</property>
    <interpolation>
      <entry><ind> -0.75 </ind><dep> -15 </dep></entry>
      <entry><ind>  0.00 </ind><dep>   0 </dep></entry>
      <entry><ind>  0.75 </ind><dep>  15 </dep></entry>
    </interpolation>
    <axis>
      <x1-m>  1.51934 </x1-m>
      <y1-m>  0.79660 </y1-m>
      <z1-m> -0.17577 </z1-m>
      <x2-m>  1.40612 </x2-m>
      <y2-m>  1.98334 </y2-m>
      <z2-m> -0.22685 </z2-m>
    </axis>
  </animation>
  
  <!-- Gouverne de direction -->
  <animation>
    <type>rotate</type>
    <object-name>direction</object-name>
    <property>surface-positions/rudder-pos-norm</property>
    <factor> 15 </factor>
    <axis>
      <x1-m> 1.53822 </x1-m>
      <y1-m> 0.00000 </y1-m>
      <z1-m> 0.03764 </z1-m>
      <x2-m> 1.53109 </x2-m>
      <y2-m> 0.00000 </y2-m>
      <z2-m> 0.66890 </z2-m>
    </axis>
  </animation>
  
  <!-- Volets -->
  <animation>
    <type>rotate</type>
    <object-name>volet</object-name>
    <property>surface-positions/flap-pos-norm</property>
    <factor> 20 </factor>
    <center>
      <x-m>  1.40239 </x-m>
      <y-m>  0.00000 </y-m>
      <z-m> -0.30498 </z-m>
    </center>
    <axis>
      <x> 0 </x>
      <y> 1 </y>
      <z> 0 </z>
    </axis>
  </animation>
  
  <!-- Compression -->
  <animation>
    <type>rotate</type>
    <object-name>axeG</object-name>
    <object-name>cacheG</object-name>
    <object-name>intcacheG</object-name>
    <object-name>roueG</object-name>
    <property>gear/gear[0]/compression-norm</property>
    <interpolation>
      <entry><ind> 0 </ind><dep>  2 </dep></entry>
      <entry><ind> 1 </ind><dep> -6 </dep></entry>
    </interpolation>
    <center>
      <x-m> -0.51225 </x-m>
      <y-m> -0.38705 </y-m>
      <z-m> -0.25912 </z-m>
    </center>
    <axis>
      <x> 1 </x>
      <y> 0 </y>
      <z> 0 </z>
    </axis>
  </animation>
  
  <animation>
    <type>rotate</type>
    <object-name>axeD</object-name>
    <object-name>cacheD</object-name>
    <object-name>intcacheD</object-name>
    <object-name>roueD</object-name>
    <property>gear/gear[1]/compression-norm</property>
    <interpolation>
      <entry><ind> 0 </ind><dep> -2 </dep></entry>
      <entry><ind> 1 </ind><dep>  6  </dep></entry>
    </interpolation>
    <center>
      <x-m> -0.51225 </x-m>
      <y-m>  0.38705 </y-m>
      <z-m> -0.25912 </z-m>
    </center>
    <axis>
      <x> 1 </x>
      <y> 0 </y>
      <z> 0 </z>
    </axis>
  </animation>
  
  <animation>
    <type>rotate</type>
    <object-name>bequille</object-name>
    <property>gear/gear[2]/compression-norm</property>
    <interpolation>
      <entry><ind> 0 </ind><dep> -2 </dep></entry>
      <entry><ind> 1 </ind><dep>  4  </dep></entry>
    </interpolation>
    <center>
      <x-m>  0.86459 </x-m>
      <y-m>  0.00000 </y-m>
      <z-m> -0.38097 </z-m>
    </center>
    <axis>
      <x> 1 </x>
      <y> 0 </y>
      <z> 0 </z>
    </axis>
  </animation>
  
  <!-- Rotation des roues lors du roulage -->
  <animation>
    <type>spin</type>
    <object-name>roueG</object-name>
    <property>gear/gear[0]/rollspeed-ms</property>
    <factor> 30 </factor>
    <center>
      <x-m> -0.933 </x-m>
      <y-m> -0.777 </y-m>
      <z-m> -1.103 </z-m>
    </center>
    <axis>
      <x>  0 </x>
      <y> -1 </y>
      <z>  0 </z>
    </axis>
  </animation>
  
  <animation>
    <type>spin</type>
    <object-name>roueD</object-name>
    <property>gear/gear[1]/rollspeed-ms</property>
    <factor> 30 </factor>
    <center>
      <x-m> -0.933 </x-m>
      <y-m>  0.777 </y-m>
      <z-m> -1.103 </z-m>
    </center>
    <axis>
      <x>  0 </x>
      <y> -1 </y>
      <z>  0 </z>
    </axis>
  </animation>
  
  <!-- Canopy -->
  <animation>
    <name>CanopyComplete</name>
    <object-name>tourvitres</object-name>
    <object-name>vitres</object-name>
  </animation>
  
  <animation>
    <type>pick</type>
    <object-name>CanopyComplete</object-name>
    <visible>true</visible>
    <action>
      <button>0</button>
      <repeatable>false</repeatable>
      <binding>
        <command>nasal</command>
        <script>doors.crew.toggle();</script>
      </binding>
    </action>
  </animation>
  
  <animation>
    <type>rotate</type>
    <object-name>CanopyComplete</object-name>
    <property>sim/model/door-positions/crew/position-norm</property>
    <interpolation>
      <entry><ind> 0 </ind><dep>  0 </dep></entry>
      <entry><ind> 1 </ind><dep> 50 </dep></entry>
    </interpolation>
    <center>
      <x-m> -0.00099 </x-m>
      <y-m>  0.00000 </y-m>
      <z-m>  0.44337 </z-m>
    </center>
    <axis>
      <x> 0 </x>
      <y> 1 </y>
      <z> 0 </z>
    </axis>
  </animation>
  
  <!-- Ajout des effets lumineux -->
  <model>
    <path>Aircraft/Arup-S2/Models/lights/beacon.xml</path>
    <offsets>
      <x-m> 1.489 </x-m>
      <y-m> 0.000 </y-m>
      <z-m> 0.955 </z-m>
    </offsets>
  </model>
  
  <model>
    <path>Aircraft/Arup-S2/Models/lights/RedLight.xml</path>
    <offsets>
      <x-m> -0.940 </x-m>
      <y-m> -3.493 </y-m>
      <z-m> -0.140 </z-m>
    </offsets>
  </model>
  
  <model>
    <path>Aircraft/Arup-S2/Models/lights/StrobeLight.xml</path>
    <offsets>
      <x-m> -0.935 </x-m>
      <y-m> -3.493 </y-m>
      <z-m> -0.140 </z-m>
    </offsets>
  </model>
  
  <model>
    <path>Aircraft/Arup-S2/Models/lights/GreenLight.xml</path>
    <offsets>
      <x-m> -0.940 </x-m>
      <y-m>  3.493 </y-m>
      <z-m> -0.140</z-m>
    </offsets>
  </model>
  
  <model>
    <path>Aircraft/Arup-S2/Models/lights/StrobeLight.xml</path>
    <offsets>
      <x-m> -0.935 </x-m>
